<?xml version="1.0" encoding="utf-8"?>
<TableLayout android:id="@+id/multichoice_iorow_table"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:orientation="vertical" android:stretchColumns="0"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<TableRow android:id="@+id/boolean_iorow_table_row" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:orientation="horizontal">
		<TextView android:id="@+id/fieldDescription"
			android:layout_width="wrap_content" android:layout_height="40px"
			android:padding="3dip" android:textSize="18sp">
		</TextView>
		<Spinner android:id="@+id/fieldValue" 
			android:layout_width="fill_parent"
			android:layout_height="wrap_content">
		</Spinner>
	</TableRow>
</TableLayout>
